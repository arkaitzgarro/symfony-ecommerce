{
    "name": "arkaitzgarro/symfony-ecommerce",
    "license": "MIT",
    "type": "project",
    "description": "Symfony base project for e-commerce",
    "authors": [
        {
            "name": "Arkaitz Garro",
            "email": "hola@arkaitzgarro.com"
        },
        {
            "name": "Elcodi Community",
            "homepage": "https://github.com/elcodi/elcodi/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "2.6.*",
        "symfony/assetic-bundle": "~2.4",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.7",

        "doctrine/common": "~2.4",
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "~1.3",

        "sonata-project/admin-bundle": "2.3.1",
        "sonata-project/doctrine-orm-admin-bundle": "2.3.0",

        "twig/extensions": "~1.2",
        "sensio/distribution-bundle": "~3.0.12",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.1",
	    "lexik/translation-bundle": "dev-master"
    },
    "require-dev": {
        "mmoreram/php-formatter": "~1.0",
        "raulfraile/ladybug-bundle": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "autoload": {
        "psr-4": {
            "QBH\\": "src/QBH"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
